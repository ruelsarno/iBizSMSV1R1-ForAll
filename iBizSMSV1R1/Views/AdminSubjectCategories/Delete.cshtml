@model iBizSMSV1R1.Models.SubjectCategory

@{
    ViewData["Title"] = "Delete Subject Category";
    Layout = "~/Views/Shared/_LayoutClassroomAdmin.cshtml";
}

<div class="container">
    <h6 id="module-views">@ViewData["Title"] : </h6>
</div>
<hr />
<span class="remove-item">Are you sure you want to delete this?</span>
<div class="Form-Body">
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.category)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.category)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="recno" />
        <input type="submit" value="Delete" class="btn btn-danger button-small" /> |
        <a asp-action="Index" class="btn btn-warning button-small">Cancel</a>
    </form>
</div>
